{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "phpstan/extension-installer": true,
      "php-http/discovery": true,
      "tbachert/spi": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/ctools": "^4.0.0",
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of nble.lib.unb.ca",
  "extra": {
    "enable-patching": true,
    "drupal-scaffold": {
      "locations": {
        "web-root": "./"
      }
    },
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "patches": {}
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/nble.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/markgoodyear/scrollup/archive/refs/heads/master.zip"
        },
        "extra": {
          "installer-name": "scrollup"
        },
        "name": "markgoodyear/scrollup",
        "type": "drupal-library",
        "version": "dev-master"
      },
      "type": "package"
    }
  ],
  "require": {
    "drupal/allowed_formats": "^2",
    "drupal/bootstrap4": "^3",
    "drupal/config_update": "^2",
    "drupal/core": "^10",
    "drupal/core-composer-scaffold": "^10",
    "drupal/core-recommended": "^10",
    "drupal/entity_reference_revisions": "^1",
    "drupal/externalauth": "^2",
    "drupal/field_formatter_class": "^1",
    "drupal/field_group": "<=3.4",
    "drupal/fixed_block_content": "^1",
    "drupal/fontawesome": "^2",
    "drupal/google_analytics": "^4",
    "drupal/health_check": "^2",
    "drupal/imce": "^3",
    "drupal/jquery_scrollup": "^2",
    "drupal/key": "^1",
    "drupal/linkit": "^6",
    "drupal/login_destination": "2.0-beta7",
    "drupal/noreqnewpass": "^1",
    "drupal/pathauto": "^1",
    "drupal/redis": "^1",
    "drupal/samlauth": "^3",
    "drupal/search_api": "<=1.30",
    "drupal/search_api_solr": "<=4.3.1",
    "drupal/simple_sitemap": "<4.2.1",
    "drupal/token": "^1",
    "drupal/twig_field_value": "^2",
    "drupal/twig_tweak": "^3",
    "markgoodyear/scrollup": "dev-master",
    "unb-libraries/composer-drupal-scaffold": "^10",
    "unb-libraries/saml_features": "dev-10.x-1.x"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^10"
  },
  "type": "project"
}
