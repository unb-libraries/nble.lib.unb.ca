{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of nble.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/samlauth": {
                "samlauth nocache metadata": "/build/patches/samlauth_nocache_metadata.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "unb-libraries/nble.lib.unb.ca",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "w8tcha/CKEditor-CodeMirror-Plugin",
                "version": "1.17.12",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/w8tcha/ckeditor-codemirror-plugin/archive/v1.17.12.zip"
                },
                "extra": {
                    "installer-name": "ckeditor_codemirror"
                }
            }
        }
    ],
    "require": {
        "drupal/allowed_formats": "1.3",
        "drupal/back_to_top": "1.1",
        "drupal/bootstrap4": "2.1.2",
        "drupal/ckeditor_codemirror": "2.3",
        "drupal/config_update": "1.5",
        "drupal/core": "8.9.8",
        "drupal/editor_file": "1.5",
        "drupal/entity_reference_revisions": "1.7",
        "drupal/externalauth": "1.3",
        "drupal/field_formatter_class": "1.1",
        "drupal/field_group": "3.1",
        "drupal/fixed_block_content": "1.0",
        "drupal/fontawesome": "2.16",
        "drupal/google_analytics": "2.5",
        "drupal/health_check": "1.2",
        "drupal/key": "1.10",
        "drupal/linkit": "4.3",
        "drupal/noreqnewpass": "1.2",
        "drupal/pathauto": "1.8",
        "drupal/redis": "1.4",
        "drupal/samlauth": "3.0-alpha2",
        "drupal/search_api": "1.18",
        "drupal/search_api_autocomplete": "1.3",
        "drupal/search_api_solr": "4.1.8",
        "drupal/simple_sitemap": "3.7",
        "drupal/token": "1.7",
        "drupal/twig_field_value": "1.2",
        "drupal/twig_tweak": "2.8",
        "unb-libraries/drupal-builder": "^8.0.0",
        "w8tcha/ckeditor-codemirror-plugin": "^1.17.12"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
